
@{
    ViewData["Title"] = "Asset Type Editor";
    Layout = "_Content";
}

<div class="text-center">
    <p>Compiler: <input id="compiler" value="TypeScript" onkeyup="document.getElementById('message').innerText=sayHello()" /></p>
